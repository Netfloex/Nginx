{
	"username": "username",
	"nginx": {
		"log": "$time_local $remote_addr"
	},
	"servers": {
		"example.com": {
			"auth": {
				"password": "password",
				"username": "username"
			},
			"certbot_name": "string",
			"cors": "*",
			"custom_css": "http://validurlstring",
			"custom_js": ["http://validurlarray"],
			"headers": {
				"Content-Type": "text/html"
			},
			"html": "html string",
			"locations": {
				"direct": "http://example.com",
				"indirect": {
					"proxy_pass": "http://example.com"
				}
			},
			"raw": {
				"custom": "option"
			},
			"subdomains": {
				"www": "http://example.com",

				"redirects": {
					"redirect": "/"
				},
				"returns": {
					"return": "200 ok"
				},
				"rewrites": {
					"rewrite": "^ https://example.com"
				},
				"includes": {
					"include": "src/nginx/proxy_pass.conf"
				},
				"defaultUsername": {
					"auth": "password",
					"html": "Username uses default"
				},

				"no_return_allowed": {
					"locations": {
						"/en": {
							"html": "Return not needed in subdomain"
						},
						"/withCors": {
							"cors": "https://return-not-needed-in-location"
						}
					}
				}
			},
			"websocket": true
		}
	}
}
